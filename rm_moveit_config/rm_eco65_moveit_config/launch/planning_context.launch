<launch>
  <!-- By default we do not overwrite the URDF. Change the following to true to change the default behavior -->
  <arg name="load_robot_description" default="false"/>

  <!-- The name of the parameter under which the URDF is loaded -->
  <arg name="robot_description" default="robot_description"/>

  <!-- Define the link6_type for the xacro file -->
  <arg name="link6_type" default="Link6"/>

  <!-- Load universal robot description format (URDF) -->
  <param if="$(arg load_robot_description)" name="$(arg robot_description)"  
    command="xacro  '$(find rm_description)/urdf/ECO65_6FB/rm_eco65_6fb_description.urdf.xacro'
    link6_type:=$(arg link6_type)
    "/>

  <!-- The semantic description that corresponds to the URDF -->
  <param name="$(arg robot_description)_semantic" textfile="$(find rm_eco65_moveit_config)/config/rm_eco65.srdf" />

  <!-- Load updated joint limits (override information from URDF) -->
  <group ns="$(arg robot_description)_planning">
    <rosparam command="load" file="$(find rm_eco65_moveit_config)/config/joint_limits.yaml"/>
    <rosparam command="load" file="$(find rm_eco65_moveit_config)/config/cartesian_limits.yaml"/>
  </group>

  <!-- Load default settings for kinematics; these settings are overridden by settings in a node's namespace -->
  <group ns="$(arg robot_description)_kinematics">
    <rosparam command="load" file="$(find rm_eco65_moveit_config)/config/kinematics.yaml"/>

  </group>

</launch>
